<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="8dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:gravity="start">

            <com.mikhaellopez.circularimageview.CircularImageView
                android:id="@+id/imageButtonMain"
                android:layout_width="@dimen/width20dp"
                android:layout_height="@dimen/height20dp"
                android:layout_centerVertical="true"
                android:layout_marginRight="4dp"
                android:background="@drawable/circle_shape_filled" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="120dp"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_marginLeft="8dp"
                android:layout_toEndOf="@+id/imageButtonMain"
                android:gravity="center|start"
                android:text="color value"
                android:textColor="@color/blue5"
                android:visibility="visible" />

            <com.mikhaellopez.circularimageview.CircularImageView
                android:id="@+id/imageButton1"
                android:layout_width="@dimen/width20dp"
                android:layout_height="@dimen/width20dp"
                android:layout_alignTop="@+id/imageButtonMain"
                android:layout_centerVertical="true"
                android:layout_marginEnd="4dp"
                android:layout_marginLeft="14dp"
                android:layout_marginRight="4dp"
                android:layout_marginStart="14dp"
                android:layout_toEndOf="@+id/textView"
                android:background="@drawable/circle_shape_filled" />

            <TextView
                android:id="@+id/textView1"
                android:layout_width="120dp"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_marginLeft="8dp"
                android:layout_toEndOf="@+id/imageButton1"
                android:gravity="center|start"
                android:text="color value"
                android:textColor="@color/blue5"
                android:visibility="visible" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:gravity="center_horizontal|start">

            <com.mikhaellopez.circularimageview.CircularImageView
                android:id="@+id/imageButton2"
                android:layout_width="@dimen/width20dp"
                android:layout_height="@dimen/width20dp"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="4dp"
                android:background="@drawable/circle_shape_filled" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="120dp"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_marginLeft="8dp"
                android:layout_toEndOf="@+id/imageButton2"
                android:gravity="center|start"
                android:text="color value"
                android:textColor="@color/blue5"
                android:visibility="visible" />

            <com.mikhaellopez.circularimageview.CircularImageView
                android:id="@+id/imageButton3"
                android:layout_width="@dimen/width20dp"
                android:layout_height="@dimen/width20dp"
                android:layout_alignTop="@+id/imageButton2"
                android:layout_marginEnd="4dp"
                android:layout_marginLeft="14dp"
                android:layout_marginRight="4dp"
                android:layout_marginStart="14dp"
                android:layout_toEndOf="@+id/textView2"
                android:background="@drawable/circle_shape_filled" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="120dp"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_marginLeft="8dp"
                android:layout_toEndOf="@+id/imageButton3"
                android:gravity="center|start"
                android:text="color value"
                android:textColor="@color/blue5"
                android:visibility="visible" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:gravity="center_horizontal|start">

            <com.mikhaellopez.circularimageview.CircularImageView
                android:id="@+id/imageButton4"
                android:layout_width="@dimen/width20dp"
                android:layout_height="@dimen/width20dp"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="4dp"
                android:background="@drawable/circle_shape_filled" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="120dp"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="8dp"
                android:layout_toEndOf="@+id/imageButton4"
                android:gravity="center|start"
                android:text="color value"
                android:textColor="@color/blue5" />

            <com.mikhaellopez.circularimageview.CircularImageView
                android:id="@+id/imageButton5"
                android:layout_width="@dimen/width20dp"
                android:layout_height="@dimen/width20dp"
                android:layout_alignTop="@+id/imageButton4"
                android:layout_marginEnd="4dp"
                android:layout_marginLeft="14dp"
                android:layout_marginRight="4dp"
                android:layout_marginStart="14dp"
                android:layout_toEndOf="@+id/textView4"
                android:background="@drawable/circle_shape_filled" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="120dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="8dp"
                android:layout_toEndOf="@+id/imageButton5"
                android:gravity="center|start"
                android:text="color value"
                android:textColor="@color/blue5"
                android:visibility="visible" />

        </RelativeLayout>

    </LinearLayout>

    <include
        layout="@layout/card_stack_view_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_horizontal"
        android:visibility="visible" />

    <TextView
        android:id="@+id/shadesHeader"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginBottom="4dp"
        android:gravity="center|center_horizontal"
        android:text="COLOR SHADES"
        android:textColor="@color/blue5"
        android:textSize="14sp"
        android:textStyle="bold"
        android:visibility="visible" />

    <!--<StackView-->
    <!--android:id="@+id/shadesStackView"-->
